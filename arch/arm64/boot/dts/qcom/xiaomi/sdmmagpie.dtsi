};

&pil_ipa_fw_mem {
	reg = <0 0x97900000 0 0x10000>;
};

&pil_ipa_gsi_mem {
	reg = <0 0x97910000 0 0x5000>;
};

&pil_gpu_mem {
	reg = <0 0x97915000 0 0x2000>;
};
